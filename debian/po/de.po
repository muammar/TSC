#. type: TP
#: debian/tsc.6:35
#, no-wrap
msgid "B<-d, --debug>"
msgstr "B<-d, --debug>"

#. type: Plain text
#: debian/tsc.6:38
msgid "Enable debug modes with the options : game performance."
msgstr "aktiviert Fehlersuchmodi mit den Optionen: Spielleistung."

#. type: TP
#: debian/tsc.6:38
#, no-wrap
msgid "B<-l, --level>"
msgstr "B<-l, --level>"

#. type: Plain text
#: debian/tsc.6:41
